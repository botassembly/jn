{
  "type": "source",
  "description": "Messages in inbox",
  "defaults": {
    "in": "inbox"
  },
  "params": [
    "from",
    "to",
    "subject",
    "is",
    "has",
    "after",
    "before",
    "newer_than",
    "older_than"
  ],
  "examples": [
